(defrule enfermedades(enfermedad ?e) => (printout t ?e crlf))

CLIPS> (run)

hipertension
lupus
leucemia
pancreatitis
cancer
artritis

(deftemplate signos-sintomas
  (slot enfermedad)
  (multislot signos)
  (multislot sintomas)
)

 (assert (signos-sintomas (enfermedad "hipertension") 
                          (signos "dolor de cabeza" "zumbido") 
                          (sintomas "fiebre" "fatiga")))

